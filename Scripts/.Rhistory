resumir_por_a침o <- function(df_semanal) {
df <- df_semanal %>%
mutate(
a침o = as.integer(str_extract(semana_a침o, "\\d{4}$"))
)
df_a침o <- df %>%
group_by(a침o) %>%
summarise(
casos_total = sum(n_casos, na.rm = TRUE),
edad_promedio = weighted.mean(edad_media, w = n_casos, na.rm = TRUE),
